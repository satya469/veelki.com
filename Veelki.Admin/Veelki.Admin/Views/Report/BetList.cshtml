                       value="true">
                S/R E-Soccer

                <input type="radio" id="TS_BINARY" data-categorytype="6">BINARY

            </li>
        </ul>
        <ul class="input-list">
            <li class="filterSettingModel">
                <a class="filter_setting" href="#" id="oddsDifferentialFilterBtn">
                    <img class="icon-filter" src="https://statics3.skyexch.bike/images/transparent.gif">Odds
                    Differential Filter (%)
                </a>
                <div class="filter_setting-wrap" id="oddsDifferentialFilterDialog" style="width: 280px">
                    <a class="close_pop" href="javascript:void(0);"
                       id="oddsDifferentialFilterCloseBtn">close_pop</a>
                    <h3>Odds Differential Filter</h3>
                    <div id="oddsDifferentialFilterContent" class="filter_selection-wrap" style="width: 280px">
                        <div>
                            <h4></h4>
                            <dl id="populateDl0" class="flex-center space-x-1 py-1 p-2">
                                <dd>
                                    Back &gt;=<input type="text" id="oddsDifferentialFilterBackMore"
                                                     name="oddsDifferentialFilterBackMoreVal" value=""> %
                                </dd>
                            </dl>
                            <dl id="populateDl1" class="flex-center space-x-1 py-1 p-2">
                                <dd>
                                    Back &lt;= <input type="text" id="oddsDifferentialFilterBackLess"
                                                      name="oddsDifferentialFilterBackLessVal" value=""> %
                                </dd>
                            </dl>
                            <dl id="populateDl2" class="flex-center space-x-1 py-1 p-2">
                                <dd>
                                    Lay &gt;= <input type="text" id="oddsDifferentialFilterLayMore"
                                                     name="oddsDifferentialFilterLayMoreVal" value=""> %
                                </dd>
                            </dl>
                            <dl id="populateDl3" class="flex-center space-x-1 py-1 p-2">
                                <dd>
                                    Lay &lt;= <input type="text" id="oddsDifferentialFilterLayLess"
                                                     name="oddsDifferentialFilterLayLessVal" value=""> %
                                </dd>
                            </dl>
                        </div>
                    </div>
                    <ul class="btn-wrap">
                        <li>
                            <a class="btn" id="oddsDifferentialFilterRefreshBtn" href="javascript:void(0);">
                                <img class="icon-refresh-s"
                                     src="https://statics3.skyexch.bike/images/transparent.gif">Refresh
                            </a>
                        </li>
                        <li>
                            <a class="btn-send" id="oddsDifferentialFilterSubmit"
                               href="javascript:void(0);">Confirm</a>
                        </li>
                    </ul>
                </div>
            </li>



            <li class="filter_setting-list" id="userFilterModel">
                <a class="filter_setting" href="#userFilterDialog" id="userFilterBtn">
                    <img class="icon-filter"
                         src="https://statics3.skyexch.bike/images/transparent.gif">User Filter
                </a>

                <div id="userFilterDialog" class="filter_setting-stakes-wrap" style="display: none;">
                    <a class="close_pop" href="javascript:void(0);" id="userFilterCloseBtn">close_pop</a>
                    <h3>User Filter</h3>

                    <div class="filter_selection-wrap">
                        <dl class="filter_selection-id">
                            <dd class="filter_selection-switch">
                                <a href="javascript:void(0);" id="userFilterFlag" class="toggle_off">OFF</a>
                            </dd>

                            <dt>User Level:</dt>
                            <dd>
                                <select id="userType" name="userType">


                                    <option value="0">
                                        Player
                                    </option>


                                </select>
                            </dd>

                            <dt>UserId:</dt>
                            <dd><input type="text" id="userId" size="20" maxlength="20"></dd>
                        </dl>
                    </div>
                    <ul class="btn-wrap">
                        <li><a class="btn-send" id="userFilterSubmitBtn" href="javascript:void(0);">Submit</a></li>
                    </ul>
                </div>
            </li>
        </ul>
        <!-- function -->

        <div class="function-wrap">
            <ul class="input-list">
                <div id="statusCondition">
                <li><label>Bet Status:</label></li>
                <li>
                    <select name="betStatus" id="betStatus" fdprocessedid="vfvh6h">
                        <option value="0">Unmatched</option>
                        <option value="1">Matched</option>
                        <option value="2">Settled</option>
                        <option value="3">Cancelled</option>
                        <option value="4">Voided</option>
                    </select>
                </li>
        </div>

        <li>
            <label>
                Period
            </label>
        </li>
        <li>
            <input id="startDate" class="cal-input" type="text" placeholder="YYYY-MM-DD" min="2023-02-28"
