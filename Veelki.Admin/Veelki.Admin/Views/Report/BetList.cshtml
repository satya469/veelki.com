            <input id="startTime" class="time-input " type="text" placeholder="09:00" maxlength="5"
                   fdprocessedid="cjrkid">
            to
            <input id="endDate" class="cal-input" type="text" placeholder="YYYY-MM-DD" min="2023-02-28"
