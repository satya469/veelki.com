    <script src="~/js/toastr.min.js"></script>
    <script src="~/js/sweetalert.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.2/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js "></script>
    <script src="https://cdn.datatables.net/buttons/1.6.2/js/dataTables.buttons.min.js"></script>


    <script>
        $(document).ready(function () {
            $(".menu-content > li > a ,.showLoaderCls").click(function () {
                $(".loader").show();
            });
            $("button[type='submit']").on("click", function () {
                $(".loader").show();
            });
            $(".customeDdl li > a").click(function () {
                  var dId = $(this).attr("id");
                  var data = {};
                  data.id = dId;
                  $.ajax({
                    type: "POST",
                    url: '@Url.Action("DeviceChange")',
                    data: '{deviceId: ' + JSON.stringify(data) + '}',
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (responce) {
                        var deviceInfo = responce.isDeviceChange;
                            $(".dName").text(deviceInfo.deviceName != null ? deviceInfo.deviceName : "Unknown");
                            $(".dSymbolCode").text(deviceInfo.deviceSymbolCode != null ? "("+deviceInfo.deviceSymbolCode+")" : "Unknown");
                            $(".dOnLine").text(deviceInfo.onLine != null ? deviceInfo.onLine : "Unknown");
                            $(".deviceType").text(deviceInfo.deviceType != null ? deviceInfo.deviceType : "Unknown");
                            $(".band").text(deviceInfo.band != null ? deviceInfo.band :"Unknown" );
                            $(".csq").text(deviceInfo.csq != null ? deviceInfo.csq : "Unknown");
                            $(".currentCls").text(deviceInfo.ratedCur != null ? deviceInfo.ratedCur : 0);
                            $(".voltageCls").text(deviceInfo.ratedVol != null ? deviceInfo.ratedVol : 0);
                            if (deviceInfo.status == "1:指令分闸" || deviceInfo.status == "10:定时分闸") {
                                 $("#switchBtn").removeClass("swOn").addClass("swOff");
                            }
                            else{
                                 $("#switchBtn").removeClass("swOff").addClass("swOn");
                            }
                    }
                  });
            });
        });
        $(window).on('load', function () {
            if (feather) {
                feather.replace({
                    width: 14,
                    height: 14
                });
            }
        })

        $(function () {
                        var errorMsg = "@(TempData["ErrorMsg"] as string)";
                          if (errorMsg !== "") {
                                toastr.error(errorMsg);
                          }
                        var successMsg = "@(TempData["SuccessMsg"] as string)";
                          if (successMsg !== "") {
                                toastr.success(successMsg);
                          }
        });
    </script>
